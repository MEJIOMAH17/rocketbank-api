<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true" android:height="@dimen/migration_button_height">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#ebebeb"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_focused="true" android:height="@dimen/migration_button_height">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#ebebeb"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:height="@dimen/migration_button_height">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/white"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
